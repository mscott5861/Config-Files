{
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.commandsToSkipShell": [
    "-workbench.action.terminal.focusFindWidget"
  ],
  "window.menuBarVisibility": "toggle",
  "vim.easymotion": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["f"],
      "after": ["leader", "leader", "s"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "explorer.autoReveal": true,
  "vim.easymotionMarkerBackgroundColor": "#0000FF",
  "vim.easymotionMarkerForegroundColorOneChar": "#FFF",
  "vim.easymotionMarkerForegroundColorTwoChar": "#FFF",
  "vim.easymotionMarkerFontFamily": "Arial",
  "vim.easymotionMarkerYOffset": "5",
  "vim.easymotionMarkerFontWeight": "bold",
  "window.zoomLevel": -1,
  "editor.fontFamily": "'DejaVu Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "line-thin",
  "diffEditor.renderSideBySide": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
    "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Horizon Bold"
}
