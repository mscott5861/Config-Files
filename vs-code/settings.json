{
  "workbench.colorTheme": "Night Owl",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "city-lights-icons-vsc",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.commandsToSkipShell": [
    "-workbench.action.terminal.focusFindWidget"
  ],
  "window.menuBarVisibility": "toggle",
  "vim.easymotion": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["f"],
      "after": ["leader", "leader", "s"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "explorer.autoReveal": true,
  "vim.easymotionMarkerBackgroundColor": "#0000FF",
  "vim.easymotionMarkerForegroundColorOneChar": "#FFF",
  "vim.easymotionMarkerForegroundColorTwoChar": "#FFF",
  "vim.easymotionMarkerFontFamily": "Arial",
  "vim.easymotionMarkerYOffset": "5",
  "vim.easymotionMarkerFontWeight": "bold",
  "window.zoomLevel": -1,
  "editor.fontFamily": "'DejaVu Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "line-thin",
  "diffEditor.renderSideBySide": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "sshfs.configs": [
    {
      "host": "192.168.1.222",
      "label": "shinobi-pi",
      "name": "spi",
      "privateKeyPath": "/home/empedocles/.ssh/shinobi-pi/id_rsa",
      "root": "/home/empedocles/",
      "username": "empedocles"
    },
    {
      "host": "192.168.1.250",
      "label": "nuc01",
      "name": "nuc01",
      "privateKeyPath": "/home/empedocles/.ssh/nuc_01/nuc_01_id_rsa",
      "root": "/home/empedocles/",
      "username": "empedocles"
    }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
