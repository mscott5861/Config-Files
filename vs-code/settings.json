{
    "workbench.colorTheme": "Dracula",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.terminal.focusFindWidget"        
    ],
    "window.menuBarVisibility": "toggle",
    "vim.easymotion": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["f"],
            "after": ["leader", "leader", "s"]
        }],
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "explorer.autoReveal": false,
    "vim.easymotionMarkerBackgroundColor": "#0000FF",
    "vim.easymotionMarkerForegroundColorOneChar": "#FFF",
    "vim.easymotionMarkerForegroundColorTwoChar": "#FFF",
    "vim.easymotionMarkerFontFamily": "Arial",
    "vim.easymotionMarkerYOffset": "5",
    "vim.easymotionMarkerFontWeight": "bold",
    "window.zoomLevel": -1,
    "editor.fontFamily": "'DejaVu Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "diffEditor.renderSideBySide": false
}